{{#switch state}}
	{{#case "start"}}<page-start files="{files}" counted="{counted}" filtered="{filtered}"/>{{/case}}
	{{#case "filter"}}<page-filter files="{files}" counted="{counted}" filtered="{filtered}"/>{{/case}}
	{{#case "report"}}<page-report files="{files}" counted="{counted}" filtered="{filtered}"/>{{/case}}
{{/switch}}
