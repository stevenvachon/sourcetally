{{#switch state}}
	{{#case "start"}}<page-start files="{files}" filtered-files="{filteredFiles}" states="{states}"/>{{/case}}
	{{#case "filter"}}<page-filter files="{files}" filtered-files="{filteredFiles}" states="{states}"/>{{/case}}
	{{#case "report"}}<page-report files="{files}" filtered-files="{filteredFiles}" states="{states}"/>{{/case}}
{{/switch}}
